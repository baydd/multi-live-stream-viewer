{
  "name": "livestream-dashboard",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "hls.js": "^1.4.0",
    "i18next": "^22.4.15",
    "next": "^16.0.0",
    "node-fetch": "^3.3.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-grid-layout": "^1.3.4",
    "react-hotkeys-hook": "^4.4.1",
    "react-i18next": "^12.2.2",
    "react-icons": "^4.12.0",
    "react-scripts": "5.0.1",
    "react-virtualized": "^9.22.5",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.7.2",
    "styled-components": "^5.3.9",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"node backend/index.js\"",
    "build": "npm run type-check && npm run format:check && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/next": "^8.0.7",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-virtualized": "^9.21.21",
    "@types/uuid": "^9.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^3.3.2"
  }
}
